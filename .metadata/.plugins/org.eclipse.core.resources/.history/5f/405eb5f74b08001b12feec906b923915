package Pos.Menu;

import java.util.Scanner;

public class Food {
	public String picture;
	public String name;
	public String contents;
	public int price;
	
	public String name() {
		Scanner in = new Scanner(System.in);
		System.out.println("이름을 등록하세요?");
		String name = in.next();
		if(name.equals(null)) {
			System.out.println("이름을 등록하세요?");
			name = in.next();
		}
		return name;
	}
}
