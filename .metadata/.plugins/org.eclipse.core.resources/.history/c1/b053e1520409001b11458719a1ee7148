package Pos.Menu;

import java.util.Map;
import java.util.Set;

public class DataRepository implements IMenuManage{

	@Override
	public Object Insert(Object... o) {
		Map<String, Food> foodMap = (Map<String, Food>) o[0];
		Set<String> keyset = foodMap.keySet();
		for(String key : keyset) {
			Food food = foodMap.get(key);
			System.out.println(food.getName());
			System.out.println(food.getContents()());
			System.out.println(food.getPicture()());
		}
		return null;
	}

	@Override
	public Object Delete(Object... o) {
		
		return null;
	}

	@Override
	public Object Modify(Object... o) {
		
		return null;
	}

	@Override
	public Object Select(Object... o) {
		
		return null;
	}

}
